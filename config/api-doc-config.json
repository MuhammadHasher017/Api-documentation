      "id": "best-practices",
      "sections": [
        {
          "type": "section",
          "title": "Best Practices",
          "practices": [
            {
              "title": "Use Specific Filters",
              "content": "Use specific ID filters (like JobIds, LoadIds) instead of retrieving all records for better performance."
            },
            {
              "title": "Handle Sorting",
              "content": "pass the columns for sorting in a comma separated string like 'JobName Asc, CreatedDate Desc'."
            },
            {
              "title": "Secure Your API Key",
              "content": "Never expose your API key in client-side code or public repositories."
            },
            {
              "title": "Handle Array Parameters",
              "content": "Array parameters should be sent as multiple query parameters with the same name (e.g., ?Ids=1&Ids=2&Ids=3)."
            },
            {
              "title": "Monitor Rate Limits",
              "content": "Implement exponential backoff when encountering rate limit responses."
            }
          ]
        },
